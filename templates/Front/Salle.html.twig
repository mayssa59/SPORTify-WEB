    }).addTo(carte);

    //on parcourt les différentes salles de sport
    $(document).ready(function () {
        $("#marker").click(function(){
            //$("tbody").children().remove()
            $.ajax({
                url: '{{ path('Table') }}',
                type: 'POST',
                data: {

                },
                success: function(data) {
                    console.log(data);
                    $.each(data, function (i, item) {
                        $.each(item, function (_, o) {

                                //on crée le marqueur et le popup
                                let marqueur = L.marker([ o.lat ,o.lng ]).addTo(carte);
                                marqueur.bindPopup("<p>"+o.region+"</p>");



                        });
                    });
                },

            });
        });
    });




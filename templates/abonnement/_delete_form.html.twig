<form method="post" action="{{ path('abonnement_delete', {'idAbonnement': abonnement.idAbonnement}) }}" onsubmit="return confirm('Are you sure you want to delete this subscription?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ abonnement.idAbonnement) }}">
    <br>
    <button class="btn btn-primary">Delete</button>
</form>
